   "source": [
    "class Book:\n",
    "    def __init__(self, title, author):\n",
    "        self.title = title\n",
    "        self.author = author\n",
    "\n",
    "    def describe(self):\n",
    "        print(f\"'{self.title}' by {self.author}\")\n",
    "\n",
    "book = Book(\"1984\", \"George Orwell\")\n",
    "book.describe()\n"
   ]
